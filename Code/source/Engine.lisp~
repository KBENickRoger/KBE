(in-package :gdl-user)

(define-object Engine (cylinder)
  
  :documentation
  (:author "Nick"
   :description "")
  
  :input-slots
  (engineLength engineDiameter)
  
  
  :computed-slots
  ((length (the engineLength))
   (radius (half (the engineDiameter)))
  )
  
  
  :objects
  ()
  
  
  :functions
  ())

